{
  "extends": "next/core-web-vitals",
  "settings": {
    "import/parsers": {
      "@typescript-eslint/parser": [".ts", ".tsx"]
    },
    "import/resolver": {
      "typescript": {
        "project": ["tsconfig.json", "package/tsconfig.json"]
      },
      "node": {
        "moduleDirectory": ["./"],
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  }
}
